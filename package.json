{
  "name": "whatsapp-conversation-summarizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "sync-version": "node scripts/sync-version.js",
    "stop:all": "taskkill /F /IM node.exe || exit 0",
    "dev:frontend": "npm run sync-version && next dev --turbopack",
    "dev:frontend:clean": "npm run stop:all && npm run dev:frontend",
    "build:frontend": "npm run sync-version && next build",
    "start:frontend": "next start",
    "lint:frontend": "next lint",
    "dev:backend": "cd backend && npm run dev",
    "build:backend": "cd backend && npm run build",
    "start:backend": "cd backend && npm start",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "test": "cd frontend && npm test",
    "test:watch": "cd frontend && npm run test:watch",
    "test:coverage": "cd frontend && npm run test:coverage",
    "test:unit": "cd frontend && npm run test:unit",
    "test:components": "cd frontend && npm run test:components",
    "test:integration": "cd frontend && npm run test:integration",
    "test:e2e": "cd frontend && npx playwright test",
    "test:performance": "cd frontend && node tests/performance/runner.js",
    "test:ci": "cd frontend && npm run test:ci",
    "test:update": "cd frontend && npm run test:update",
    "test:all": "cd frontend && npm-run-all --sequential test:unit test:components test:integration && npm run test:e2e",
    "test:parallel": "npm-run-all --parallel test:unit test:components test:integration"
  },
