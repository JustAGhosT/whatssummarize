{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/coverage": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/coverage": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "relative",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.packageManager": "pnpm",
  "eslint.format.enable": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.semi": true,
  "prettier.printWidth": 100,
  "prettier.bracketSpacing": true,
  "prettier.jsxSingleQuote": false,
  "prettier.arrowParens": "always",
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.endOfLine": "lf",
  "prettier.quoteProps": "as-needed",
  "prettier.proseWrap": "preserve",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.vueIndentScriptAndStyle": false,
  "prettier.embeddedLanguageFormatting": "auto",
  "prettier.singleAttributePerLine": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#ffd700",
    "editorBracketHighlight.foreground2": "#da70d6",
    "editorBracketHighlight.foreground3": "#87cefa",
    "editorBracketHighlight.foreground4": "#ffd700",
    "editorBracketHighlight.foreground5": "#da70d6",
    "editorBracketHighlight.foreground6": "#87cefa"
  },
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.hbs": "handlebars",
    "*.hjs": "handlebars",
    "*.handlebars": "handlebars"
  },
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "welcomePage",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "GitHub Dark Default",
  "terminal.integrated.fontFamily": "'Cascadia Code', 'Fira Code', 'Hack', 'Menlo', 'Monaco', 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.rightClickBehavior": "copyPaste",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.shellIntegration.history": 10000,
  "terminal.integrated.shellIntegration.showWelcome": false,
  "terminal.integrated.shellIntegration.suggestEnabled": true,
  "terminal.integrated.shellIntegration.autoRepl": true,
  "terminal.integrated.shellIntegration.autoReplTimeout": 3000,
  "terminal.integrated.shellIntegration.commandsToSkipShell": [
    "workbench.action.terminal.clear"
  ],
  "terminal.integrated.shellIntegration.cwd": "${workspaceFolder}",
  "terminal.integrated.shellIntegration.cwdDetection": true,
  "terminal.integrated.shellIntegration.cwdChangeDetection": true,
  "terminal.integrated.shellIntegration.cwdChangeDetectionInterval": 1000,
  "terminal.integrated.shellIntegration.cwdChangeDetectionThreshold": 1,
  "terminal.integrated.shellIntegration.cwdChangeDetectionTimeout": 5000,
  "terminal.integrated.shellIntegration.cwdChangeDetectionDebounce": 1000,
  "terminal.integrated.shellIntegration.cwdChangeDetectionMaxDepth": 10,
  "terminal.integrated.shellIntegration.cwdChangeDetectionExclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
    "**/CVS/**"
  ],
  "terminal.integrated.shellIntegration.cwdChangeDetectionInclude": [
    "**/*.js",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.json",
    "**/*.md",
    "**/*.html",
