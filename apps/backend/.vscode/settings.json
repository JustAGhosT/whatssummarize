    "**/*.cmd"
  ],
  "terminal.integrated.shellIntegration.cwdChangeDetectionIgnore": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
