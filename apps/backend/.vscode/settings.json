  "terminal.integrated.shellIntegration.cwdChangeDetectionUseIgnoreFiles": true,
  "terminal.integrated.shellIntegration.cwdChangeDetectionUseGlobalIgnoreFiles": true,
  "terminal.integrated.shellIntegration.cwdChangeDetectionUseParentIgnoreFiles": true,
